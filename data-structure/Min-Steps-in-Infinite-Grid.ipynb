   "source": [
    "def cover_points(A, B):\n",
    "    steps = 0\n",
    "    for i in range(len(A) -1):\n",
    "        x = abs(A[i]- A[i+1])\n",
    "        y = abs(B[i] - B[i+1])\n",
    "        steps += max(x, y)\n",
    "    \n",
    "    return steps"
   ]
  },
